extends layout

block content
    .w3-card-4
      header.w3-container.w3-teal(style="padding:15px; margin-bottom:15px;")
        h3 Elemento "#{nome}"
      if elem
        ul
          each a in elem
            li(style="padding-bottom:15px")
              b #{a.p.value.split("#")[1]} 
              if a.o.value.split("#")[1]
                |- #{a.o.value.split("#")[1]}
              else 
                |- #{a.o.value}