extends layout

block content
    .w3-card-4
      header.w3-container.w3-teal(style="padding:15px")
        h3 Utilizadores

      .div(style="margin:30px")
        each a in uts
          if a.s.value.split("#")[1]
            li(style="padding-bottom:15px") 
              a(href=`/utilizadores/${a.s.value.split("#")[1]}`) #{a.s.value.split("#")[1]}

